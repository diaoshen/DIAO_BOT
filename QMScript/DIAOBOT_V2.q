Function loadPresetToQueueListBox(fileName)

	Text = Plugin.File.ReadFileEx(Plugin.SysEx.GetDir(0) & fileName)
	Accounts = Split(Text, "|")
	Form1.queueListBox.List = ""
	If UBound(Accounts) >= 0 Then 
		i = 0
		For UBound(Accounts)
			Form1.queueListBox.AddItem Cstr(Accounts(i))
			Form1.queueListBox.ListIndex = Form1.queueListBox.ListIndex + 1
			i=i+1
		Next
	End If

End Function

/*
	Tasks  Functions 
*/ 
Function LoginEvent(time)
	// time in minutes 
	Call Print( "Starting LoginEvent for " & time & " mins")
	t1 = Plugin.Sys.GetTime()
	While Plugin.Sys.GetTime() - t1 <= (time * 60 * 1000)
		Delay 1000 * 60 // Show time every 60s
		Call Print( "Elapsed Time : " & Round((Plugin.Sys.GetTime() - t1)/1000/60,2) & "mins")
	Wend
End Function

Function RunRAR()
    Call enroll
	/* Waiting to get in */
End Function
