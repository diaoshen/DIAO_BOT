    MoveTo kMenuCornerX + raidOffsetLocX, kMenuCornerY + raidOffsetLocY// select battle
    Call Print( "Clicked Select Battle" & kMenuCornerX & "+" &  raidOffsetLocX & "," & kMenuCornerY & "+" & raidOffsetLocY)
    LeftClick 1
    Delay 3000

    MoveTo kMenuCornerX + departSoloOffsetLocX, kMenuCornerY + departSoloOffsetLocY// Depart(solo)
    Call Print( "Clicked Depart Solo" & kMenuCornerX & "+" &  departSoloOffsetLocX & "," & kMenuCornerY & "+" & departSoloOffsetLocY)
    LeftClick 1
    Delay 3000

    MoveTo kMenuCornerX + waitListRegLocX, kMenuCornerY + waitListRegLocY// Wait List Registration 
    Call Print( "Cliked WaitList Registration" & kMenuCornerX & "+" &  waitListRegLocX & "," & kMenuCornerY & "+" & waitListRegLocY)
    LeftClick 1
    Delay 3000

	
//    MoveTo wndX + arrowUpLocX, wndY + arrowUpLocY// Arrow up to show depart buton 
//    LeftClick 1
	dm_ret = dm.FindStr(wndX + 788 , wndY+ 746 , wndX + 869 , wndY + 783 , "ArrowUP" , "d8d9d9-303030|dadfda-202020" , 0.6 , X , Y)
	If X >= 0 And Y >= 0 Then 
		Call Print( "Found")
		MoveTo X,Y+5
		Delay 2000
		LeftClick 1
		Delay 2000
	End If

    
	MoveTo wndX + departNowLocX, wndY + departNowLocY // Depart Now button
    Delay 2000
    LeftClick 1

End Function


