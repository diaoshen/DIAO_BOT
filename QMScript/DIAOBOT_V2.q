Function cycle()
	Rem begin
	// Launcher Login
	Call launcherLogin(id, pw)
	Delay client_loading(Form1.delayComboBox.ListIndex)
	hwnd_vindi = get_hwnd("Vindictus", 30)
	// Game Login 
	If hwnd_vindi <> 0 Then 
		// RAR need Vindictus hwnd corners 
		dm_ret = dm.GetWindowRect(hwnd_vindi, x, y, foo, bar)
		kMenuCornerX = kMenuOffsetX + x
		kMenuCornerY = kMenuOffsetY + y
		wndX = x
		wndY = y
		Call Print( "Starting  GameLogin...")
		startTime = Plugin.Sys.GetTime()
		ret = gameLogin(id, pw, 60)//60second
		If ret = 0 Then 
			Goto failed
		End If
		Call Print( "GameLogin Complete. Took : " & (Plugin.Sys.GetTime() - startTime) / 1000 / 60 & "minutes")
	Else 
		Goto failed
	End If
	// DO TASK : TODO 
	Call Print( "Initiate task...")
	Call RunTask(Plugin.Sys.GetTime())
	//Log OFF & Cleanup
	Call Print( "Starting Cleanup sequence...")
	Call killProcess("vindictus")
	Call killProcess("launcher")
	Delay 2000
	Call startProcess("launcher")
	Exit Function
	
	
	// Failed : Recovery
	Rem failed
	Call Print( "Failed to Login  : PIN ERROR")
	Call Print( "Recovering...")
	Call killProcess("vindictus")
	Call killProcess("launcher")
	Delay launcher_loading(Form1.delayComboBox.ListIndex)
	Call startProcess("launcher")
	Delay launcher_loading(Form1.delayComboBox.ListIndex)
	Goto begin
End Function


Function LoginOnly(id)
	Call killProcess("vindictus")
	Call killProcess("launcher")
	Call startProcess("launcher")
	Delay launcher_loading(Form1.delayComboBox.ListIndex)
	t0 = Plugin.Sys.GetTime()
	Rem begin
	// Launcher Login
	Call launcherLogin(id, pw)
	Delay client_loading(Form1.delayComboBox.ListIndex)
	hwnd_vindi = get_hwnd("Vindictus", 30)
	// Game Login 
	If hwnd_vindi <> 0 Then 
		Call Print( "Starting  GameLogin...")
		startTime = Plugin.Sys.GetTime()
		ret = gameLogin(id, pw, 60)//60second
		If ret = 0 Then 
			Goto failed
		End If
		Call Print( "GameLogin Complete. Took : " & (Plugin.Sys.GetTime() - startTime)/1000/60 & "minutes")
	End If
	Exit Function
	
	
	// Failed : Recovery
	Rem failed
	Call Print( "Failed to Login  : PIN ERROR")
	Call Print( "Recovering...")
	Call killProcess("vindictus")
	Call killProcess("launcher")
	Delay launcher_loading(Form1.delayComboBox.ListIndex)
	Call startProcess("launcher")
	Delay launcher_loading(Form1.delayComboBox.ListIndex)
	Goto begin	
	
	
	Call Print( "Login has completed , time used =  " & Round((Plugin.Sys.GetTime() - t0) / 1000 / 60 , 2) & "mins")
End Function



/**
	Utility Functions
*/

Function RunTask(startTime)
	If rar_t > 0 Then 
		Call Print( "Need to run rar for " & rar_t & " times")
		Call RunRAR()
	End If
	If coin_t > 0 Then 
		Call Print( "Need to run coin for" & coin_t & " times")
	End If
	If login_t > 0 Then 
		elapsedTime = (Plugin.Sys.GetTime() - startTime)/1000/60
		remainTime = login_t - elapsedTime
		Call LoginEvent(remainTime)
	End If
	Call Print( "All Task Completed")
End Function

Function killProcess(name)
	cmdString = ""
	Select Case name
		Case "vindictus"
			cmdString = "cmd /c taskkill /F /IM Vindictus.exe"
		Case "launcher"
			cmdString = "cmd /c taskkill /F /IM nexon_client.exe"
	End Select
	Call Print( cmdString)
	RunApp cmdString	
End Function

Function startProcess(name)
	cmdString = ""
	Select Case name
		Case "launcher"
			//cmdString =
			If Form1.path1OptionBox.Value = 1 Then 
				cmdString = "C:\Program Files (x86)\Nexon\Nexon Launcher\nexon_launcher.exe"
			Else 
				cmdString = "E:\games\Nexon Launcher\nexon_launcher.exe"
			End If
			
	End Select
	Call Print( cmdString)
	RunApp cmdString
End Function

Function launcherLogin(id, pw)
	hwnd = dm.FindWindow("", "Nexon Global Launcher")
	
	If hwnd = 0 Then 
	 	Call Print( "Cannot find nexon launcher")
	End If
	//dm_ret = dm.SetWindowState(hwnd,0) 
	dm_ret = dm.GetClientRect(hwnd,x1,y1,x2,y2)
	// Login 
	dm_ret = dm.FindStr(x1, y1, x2, y2, "id", "5c5c5c-202020", 0.8, X, Y)
	If X >= 0 And Y >= 0 Then 
		MoveTo x, y
		LeftClick 1
		Dim ch
		//Type email
		Call Print( "Typing id...")
		For i = 1 To Len(id)	
			ch = Mid(id, i, 1)
			If ch = "@" Then 
				KeyDown 16, 1
				KeyPress 50, 1
				KeyUp 16, 1
			Else 
				KeyPress ch , 1
			End If
			Delay 100
		Next
		Delay 300
		KeyPress "Tab", 1
		Delay 300
		//Type password
		Call Print( "Typing password...")
		For i = 1 To Len(pw)
			ch = Mid(pw, i, 1)
			KeyPress ch, 1
			Delay 100
		Next
		Delay 300
		//Login
		KeyPress "Enter", 1
		//Play
		MoveTo 0,0
		Delay playButton_loading(Form1.delayComboBox.ListIndex)
		dm_ret = dm.FindStr(x1, y1, x2, y2, "play", "000000-303030|09473d-202020", 0.8, X, Y)
		If X >= 0 And Y >= 0 Then 
			MoveTo X, Y
			Call Print( "Clicked Play.")
			LeftClick 1
		End If
	End If
End Function
	
Function gameLogin(id, pass, timeout)
dm_ret = dm.GetWindowRect(hwnd_vindi,x1, y1, x2, y2)
kMenuCornerX = kMenuOffsetX + x1
kMenuCornerY = kMenuOffsetY + y1
wndX = x1
wndY = y1
Call Print( "Vindictus hwnd : " & hwnd_vindi)
Call Print( "Vindicus Corner : " & wndX & "," & wndY )
// From start game button to In-Game
t1 = Plugin.Sys.GetTime()
Do while Plugin.Sys.GetTime() - t1 <= (timeout * 1000)
	dm_ret = dm.FindStr(x1, y1, x2, y2, "sg", "1d1d1d-303030|000000-202020", 0.8, intX, intY)
	If intX >= 0 And intY >= 0 Then 
		MoveTo intX, intY
		LeftClick 1 // Start Game
		Delay 2000
		dm_ret = dm.FindStr(x1, y1, x2, y2, "ok", "574a3d-101010|887059-202020", 0.8, intX, intY)
		If intX >= 0 And intY >= 0 Then 
			MoveTo intX, intY
			LeftClick 1 // OK 
			Delay 4000
			KeyPress "Enter", 1 // Different IP Box
			Delay 2000
			// Pin , 30s timeout 
			t1 = Plugin.Sys.GetTime()
			dm_ret = dm.FindStr(x1, y1, x2, y2, "&", "90785f-101010|8f765e-101010|71604e-101010|-202020|-101010", 0.8, intX, intY)
			While dm_ret = -1 And Plugin.Sys.GetTime() - t1 <= 30000
				dm_ret = dm.FindStr(x1, y1, x2, y2, "&", "90785f-101010|8f765e-101010|71604e-101010|-202020|-101010", 0.8, intX, intY)
				Call Print( "Failed to find & ")
				Delay 2000
			Wend
			If intX >= 0 And intY >= 0 Then 
				MoveTo intX, intY
				LeftClick 6
				dm_ret = dm.FindStr(x1, y1, x2, y2,"ok2","806b55-101010|5f5041-202020|927960-202020",0.8,intX,intY)
				If intX >= 0 And intY >= 0 Then 
					MoveTo intX, intY
					LeftClick 1
					Delay 2000
					MoveTo x2 - 60, y1 + 75
					LeftClick 3' Click character 
					Call Print( "loading...")
					Delay charToInGame_loading(Form1.delayComboBox.ListIndex) //Should be in game within 30s
					Call exitInn()
					Call Print( "ExitInn() Complete")
					gameLogin = 1 // Return 1 as Successful
					Exit Function
				End If
			End If
			Delay 2000
			Call Print( "Can't find &  and timeout is reached")
			gameLogin = 0
			Exit Function
		End If
	End If
	Delay 2000
Loop
Call Print( "GameLogin() TimedOut")
// TODO Error Recovery for GameLogin Fails
End Function

Function get_hwnd(name, timeout)
	t1 = Plugin.Sys.GetTime()
	Do While Plugin.Sys.GetTime() - t1 <= timeout*1000
		temp_hwnd = Plugin.Window.Find(0,name)
		Call Print( "Waiting for " & name)
		Delay 3000
		If temp_hwnd <> 0 Then 
			get_hwnd = temp_hwnd
			Exit Function
		End If
	Loop
	get_hwnd = 0
End Function

Function exitInn()
	Call Print( "Exiting Inn")
	KeyPress "Enter" , 1
	KeyPress "ESC", 1
	Call Print( "Pressed Esc to quit race event")
	t1 = Plugin.Sys.GetTime()
	Do While Plugin.Sys.GetTime() - t1 <= 4000
		KeyPress "Ctrl" , 1
		Delay 500
	Loop
	Call Print( "Pressing ESC to escape Inn")
	KeyPress "ESC", 4
	If GetCursorShape(0) = 2132794836 Then 
		Call Print( "Switch mouse mode to camera mode")
		KeyPress "ESC" , 1
	End If
End Function

