Event Form1.loadAccountBtn.Click
	path = Plugin.SysEx.GetDir(0) & "\accounts.txt"
	IsFile = Plugin.File.IsFileExist(path)
	If IsFile = True Then 
		Form1.accountListBox.List = ""
		FileContent = Plugin.File.ReadFileEx(path)
		accounts = Split(FileContent, "|")
		If UBound(accounts) >= 0 Then 
			i = 0
			For UBound(accounts)
				Form1.accountListBox.AddItem Cstr(accounts(i))
				Form1.accountListBox.ListIndex = Form1.accountListBox.ListIndex + 1
				i=i+1
			Next
		Else 
			Call Print( "accounts.txt is empty")
		End If
	Else 
		Call Print( "accounts.txt doesn't exist")
	End If
End Event

